{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",
    "sourceMap": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "rootDir": ".",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "types": ["vite/client","node"],
    "typeRoots": [ "../node_modules/@types" ]
  },
  "include": [
    "src"
  ]
}
